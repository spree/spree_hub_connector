<fieldset class="no-border-top">
  <legend>Endpoint Test</legend>

  <div class="error-message"></div>

  <div class="row">
    <div class="left six columns alpha">
      <div class="field">
        <label for="endpoint-message-samples">Sample</label>
        <select id="endpoint-message-samples" class="fullwidth">
        </select>
      </div>
    </div>

    <div class="right six columns omega">
      <div class="field">
        <label for="endpoint-message-samples">Last generated message:</label>
        <p id="endpoint-last-generated-message"></p>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="field">
      <label for="endpoint-message-editor">Message</label>
      <div id="endpoint-message-editor">{}</div>
    </div>
  </div>

  <div class="form-buttons filter-actions actions">
    <button id="create-endpoint-message" class="icon-refresh button" type="submit">Create Message</button>
    <span class="or">or</span>
    <button id="cancel" class="button icon_link with-tip icon-remove">Cancel</button>
  </div>
</fieldset>
